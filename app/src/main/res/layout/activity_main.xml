<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.root.testapplicationo.MainActivity">

    <Spinner
        android:id="@+id/home_select_category_spinnner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    </Spinner>

    <Spinner
        android:id="@+id/home_select_sub_category_spinnner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    </Spinner>

    <Spinner
        android:id="@+id/home_select_product_spinnner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    </Spinner>

    <TextView
        android:id="@+id/home_token"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/set_token"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        />
</LinearLayout>
