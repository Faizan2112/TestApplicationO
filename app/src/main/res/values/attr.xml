<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DiscreteScrollView">
        <attr name="dsv_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="FeaturedRecyclerView">
        <attr name="defaultItemHeight" format="dimension" />
        <attr name="featuredItemHeight" format="dimension" />
        <attr name="snapGravity" format="integer">
            <enum name="start" value="0" />
            <enum name="center" value="1" />
            <enum name="end" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="EditSpinner">
        <attr name="dropDownDrawable" format="reference" />
        <attr name="dropDownDrawableWidth" format="dimension" />
        <attr name="dropDownDrawableHeight" format="dimension" />
        <attr name="dropDownDrawableSpacing" format="dimension" />
        <attr name="dropDownAnimStyle" format="reference" />
        <attr name="dropDownAnchor" format="integer" />
        <attr name="dropDownWidth" format="dimension" />
        <attr name="dropDownHeight" format="dimension" />
        <attr name="dropDownSelector" format="reference" />
    </declare-styleable>

    <attr name="icvp_min_page_scale_offset" format="dimension" />
    <attr name="icvp_center_page_scale_offset" format="dimension" />
    <attr name="icvp_min_page_scale" format="float" />
    <attr name="icvp_max_page_scale" format="float" />
    <attr name="icvp_medium_scaled" format="boolean" />
    <attr name="icvp_scroll_duration" format="integer" />
    <attr name="icvp_page_duration" format="integer" />
    <attr name="icvp_interpolator" format="reference" />
    <declare-styleable name="HorizontalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset" />
        <attr name="icvp_center_page_scale_offset" />
        <attr name="icvp_min_page_scale" />
        <attr name="icvp_max_page_scale" />
        <attr name="icvp_medium_scaled" />
        <attr name="icvp_scroll_duration" />
        <attr name="icvp_page_duration" />
        <attr name="icvp_interpolator" />
    </declare-styleable>
    <declare-styleable name="VerticalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset" />
        <attr name="icvp_center_page_scale_offset" />
        <attr name="icvp_min_page_scale" />
        <attr name="icvp_max_page_scale" />
        <attr name="icvp_medium_scaled" />
        <attr name="icvp_scroll_duration" />
        <attr name="icvp_page_duration" />
        <attr name="icvp_interpolator" />
    </declare-styleable>
    <declare-styleable name="FeatureCoverFlow">
        <attr name="coverWidth" format="dimension" />
        <attr name="coverHeight" format="dimension" />
        <attr name="spacing" format="float" />
        <attr name="rotationThreshold" format="float" />
        <attr name="scalingThreshold" format="float" />
        <attr name="adjustPositionThreshold" format="float" />
        <attr name="adjustPositionMultiplier" format="float" />
        <attr name="maxRotationAngle" format="float" />
        <attr name="maxScaleFactor" format="float" />
        <attr name="circlePathRadius" format="float" />
        <attr name="circlePathRadiusInMatrixSpace" format="float" />
        <attr name="reflectionHeight" format="float" />
        <attr name="reflectionGap" format="dimension" />
        <attr name="reflectionOpacity" format="integer" />
        <attr name="tunningWidgetSize" format="dimension" />
        <attr name="alignAnimationTime" format="integer" />
        <attr name="verticalPaddingTop" format="dimension" />
        <attr name="verticalPaddingBottom" format="dimension" />
        <attr name="reflectionBackroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="EndlessLoopAdapterContainer">
        <attr name="shouldRepeat" format="boolean" />
    </declare-styleable>
    <declare-styleable name="BasicContentBand">
        <attr name="gridMode">
            <flag name="fixed" value="0" />
            <flag name="dynamic" value="1" />
        </attr>
        <attr name="deviceSpecificPixelSize" format="integer" />
    </declare-styleable>
</resources>