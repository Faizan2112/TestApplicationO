        <activity android:name=".databindingtest.DataBindTestActivity" />
        <activity android:name=".retrotest.RetroTest" />
        <activity android:name=".anewhome.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".anewhome.BaseActivity"></activity>
    </application>

</manifest>